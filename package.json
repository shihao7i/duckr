{
  "name": "Redux-Immutable",
  "version": "1.0.0",
  "description": "<img src=\"http://www.reactjsprogram.com/images/reactjsprogram-500.png\" width=\"250\" align=\"right\">",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "production": "webpack -p",
    "lint": "eslint app/.; exit 0",
    "fix": "eslint --fix app/.; exit 0",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "^containers$": "<rootDir>/app/containers/index.js",
      "^components$": "<rootDir>/app/components/index.js",
      "^redux/modules/usersLikes$": "<rootDir>/app/redux/modules/usersLikes.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReactjsProgram/Redux-Immutable.git"
  },
